<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="Style_Default_Fragment_MapBottom_TabWidget">
        <item name="android:textSize">@dimen/default_font_size_t28</item>
        <item name="android:textColor">@color/default_font_color_c33</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">6.0dip</item>
